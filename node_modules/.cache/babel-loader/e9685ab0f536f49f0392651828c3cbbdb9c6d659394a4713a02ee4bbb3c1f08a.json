{"ast":null,"code":"export default {\n  props: {\n    users: {\n      type: Array,\n      required: true\n    },\n    peopleList: {\n      type: Array,\n      required: true\n    }\n  },\n  methods: {\n    editNote: function (id) {\n      this.$emit('editNote', id);\n    },\n    deleteNote: function (id) {\n      console.log('delete', id);\n    },\n    toggleEdit: function (user) {\n      this.$store.dispatch('toggleEditMode', user.id);\n      if (!this.$store.state.editMode[user.id]) {\n        this.saveName(user);\n      }\n    },\n    saveName: function (user) {\n      const newName = this.editName[user.id];\n      this.$store.dispatch('saveUserName', {\n        id: user.id,\n        name: newName\n      });\n    }\n  },\n  computed: {\n    editName: function () {\n      const editName = {};\n      this.users.forEach(function (user) {\n        editName[user.id] = user.name;\n      });\n      return editName;\n    }\n  }\n};","map":{"version":3,"names":["props","users","type","Array","required","peopleList","methods","editNote","id","$emit","deleteNote","console","log","toggleEdit","user","$store","dispatch","state","editMode","saveName","newName","editName","name","computed","forEach"],"sources":["D:\\Ira\\Program\\list\\src\\components\\viewList.vue"],"sourcesContent":["\r\n<script>\r\nexport default {\r\n  props: {\r\n    users: {\r\n      type: Array,\r\n      required: true\r\n    },\r\n    peopleList: {\r\n      type: Array,\r\n      required: true\r\n    }\r\n  },\r\n  methods: {\r\n    editNote: function(id) {\r\n      this.$emit('editNote', id);\r\n    },\r\n    deleteNote: function(id) {\r\n      console.log('delete', id);\r\n    },\r\n    toggleEdit: function(user) {\r\n      this.$store.dispatch('toggleEditMode', user.id);\r\n      if (!this.$store.state.editMode[user.id]) {\r\n        this.saveName(user);\r\n      }\r\n    },\r\n    saveName: function(user) {\r\n      const newName = this.editName[user.id];\r\n      this.$store.dispatch('saveUserName', { id: user.id, name: newName });\r\n    }\r\n  },\r\n  computed: {\r\n    editName: function() {\r\n      const editName = {};\r\n      this.users.forEach(function(user) {\r\n        editName[user.id] = user.name;\r\n      });\r\n      return editName;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n<style>\r\n.icon {\r\n  width: 20px;\r\n  height: 20px;\r\n}\r\n</style>\r\n<template>\r\n  <table class=\"table mx-4\">\r\n    <thead>\r\n      <tr class=\"row\">\r\n        <th class=\"col-1\">#</th>\r\n        <th class=\"col-2\">Name</th>\r\n        <th class=\"col-2\">Family Name</th>\r\n        <th class=\"col-2\">Experience</th>\r\n        <th class=\"col-2\">Age</th>\r\n        <th class=\"col-2\">Address</th>\r\n        <th class=\"col-1\">Actions</th>\r\n      </tr>\r\n    </thead>\r\n    <tbody>\r\n      <tr v-for=\"(person, index) in peopleList\" :key=\"person.id\" class=\"row\">\r\n        <th class=\"col-1\">{{ index + 1 }}</th>\r\n        <td class=\"col-2\">{{ person.name }}</td>\r\n        <td class=\"col-2\">{{ person.familyName }}</td>\r\n        <td class=\"col-2\">{{ person.experience }}</td>\r\n        <td class=\"col-2\">{{ person.age }}</td>\r\n        <td class=\"col-2\">{{ person.address }}</td>\r\n        <td class=\"col-1 d-flex flex-row\">\r\n          <img\r\n            src=\"../assets/edit.png\"\r\n            class=\"icon d-flex justify-content-end\"\r\n            @click=\"editNote(person.id)\"\r\n          />\r\n          <img\r\n            src=\"../assets/delete.png\"\r\n            class=\"icon d-flex justify-content-end\"\r\n            @click=\"deleteNote(person.id)\"\r\n          />\r\n        </td>\r\n      </tr>\r\n\r\n      <tr v-for=\"(user, index) in users\" :key=\"user.id\" class=\"row\">\r\n        <th class=\"col-1\">{{ index + 1 }}</th>\r\n        <td class=\"col-2\">\r\n          <template v-if=\"!$store.state.editMode[user.id]\">\r\n            {{ user.name }}\r\n            <img\r\n              src=\"../assets/edit.png\"\r\n              class=\"icon d-flex justify-content-end\"\r\n              @click=\"toggleEdit(user)\"\r\n            />\r\n          </template>\r\n          <template v-else>\r\n            <input v-model=\"editName[user.id]\" />\r\n            <img\r\n              src=\"../assets/check.png\"\r\n              class=\"icon d-flex justify-content-end\"\r\n              @click=\"toggleEdit(user)\"\r\n            />\r\n          </template>\r\n        </td>\r\n        <td class=\"col-2\">{{ user.familyName }}</td>\r\n        <td class=\"col-2\">{{ user.experience }}</td>\r\n        <td class=\"col-2\">{{ user.age }}</td>\r\n        <td class=\"col-2\">{{ user.address }}</td>\r\n        <td class=\"col-1 d-flex flex-row\">\r\n          <img\r\n            src=\"../assets/delete.png\"\r\n            class=\"icon d-flex justify-content-end\"\r\n            @click=\"deleteNote(user.id)\"\r\n          />\r\n        </td>\r\n      </tr>\r\n    </tbody>\r\n  </table>\r\n</template>\r\n\r\n<style>\r\n.icon {\r\n  width: 20px;\r\n  height: 20px;\r\n}\r\n\r\n</style>\r\n"],"mappings":"AAEA,eAAe;EACbA,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC,KAAK;MACXC,QAAQ,EAAE;IACZ,CAAC;IACDC,UAAU,EAAE;MACVH,IAAI,EAAEC,KAAK;MACXC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDE,OAAO,EAAE;IACPC,QAAQ,EAAE,SAAAA,CAASC,EAAE,EAAE;MACrB,IAAI,CAACC,KAAK,CAAC,UAAU,EAAED,EAAE,CAAC;IAC5B,CAAC;IACDE,UAAU,EAAE,SAAAA,CAASF,EAAE,EAAE;MACvBG,OAAO,CAACC,GAAG,CAAC,QAAQ,EAAEJ,EAAE,CAAC;IAC3B,CAAC;IACDK,UAAU,EAAE,SAAAA,CAASC,IAAI,EAAE;MACzB,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,gBAAgB,EAAEF,IAAI,CAACN,EAAE,CAAC;MAC/C,IAAI,CAAC,IAAI,CAACO,MAAM,CAACE,KAAK,CAACC,QAAQ,CAACJ,IAAI,CAACN,EAAE,CAAC,EAAE;QACxC,IAAI,CAACW,QAAQ,CAACL,IAAI,CAAC;MACrB;IACF,CAAC;IACDK,QAAQ,EAAE,SAAAA,CAASL,IAAI,EAAE;MACvB,MAAMM,OAAM,GAAI,IAAI,CAACC,QAAQ,CAACP,IAAI,CAACN,EAAE,CAAC;MACtC,IAAI,CAACO,MAAM,CAACC,QAAQ,CAAC,cAAc,EAAE;QAAER,EAAE,EAAEM,IAAI,CAACN,EAAE;QAAEc,IAAI,EAAEF;MAAQ,CAAC,CAAC;IACtE;EACF,CAAC;EACDG,QAAQ,EAAE;IACRF,QAAQ,EAAE,SAAAA,CAAA,EAAW;MACn